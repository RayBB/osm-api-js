{
  "name": "osm-api",
  "version": "1.0.2",
  "contributors": [
    "Kyle Hensel (https://github.com/k-yle)"
  ],
  "description": "🗺️🌏 Javascript/Typescript wrapper around the OpenStreetMap API",
  "main": "dist",
  "unpkg": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [
    "osm",
    "openstreetmap",
    "openstreetmap api"
  ],
  "repository": "https://github.com/k-yle/osm-api-js",
  "scripts": {
    "build": "rm -rf dist && tsc --emitDeclarationOnly && rm -rf dist/__tests__ && browserify -x isomorphic-fetch -s OSM src/index.ts -p tsify > dist/index.js && uglifyjs dist/index.js > dist/index.min.js",
    "lint": "eslint --format pretty --ext .ts --ignore-path .gitignore . --resolve-plugins-relative-to ./node_modules/eslint-config-kyle",
    "test": "jest --runInBand",
    "trypublish": "npm publish || true"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "fast-xml-parser": "^4.0.0-beta.8",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "browserify": "^17.0.0",
    "eslint": "^8.4.1",
    "eslint-config-kyle": "^7.10.1",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.1",
    "tsify": "^5.0.4",
    "typescript": "^4.5.4",
    "uglify-js": "^3.14.5"
  },
  "eslintConfig": {
    "extends": "kyle"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "collectCoverage": true
  }
}
